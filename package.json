{
  "scripts": {
    "build-css": "tailwindcss -i src/app.css -o public/assets/css/build.css --minify",
    "start-css": "tailwindcss -i src/app.css -o public/assets/css/build.css --minify --watch",
    "build-js": "webpack",
    "build-sprite": "svg-sprite --symbol --symbol-dest public/assets/img --symbol-sprite sprite.svg src/svg/*.svg",
    "build": "npm run build-css && webpack && npm run build-sprite"
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "svg-sprite": "^2.0.4",
    "tailwindcss": "^3.4.4",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.36",
    "air-datepicker": "^3.5.3"
  }
}
